# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA_INSTALL=true
VITE_ENABLE_OFFLINE_MODE=true

# Production Configuration
VITE_DEV_TOOLS=false
VITE_LOG_LEVEL=error
